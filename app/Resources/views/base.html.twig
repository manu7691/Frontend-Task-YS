    <header>
        <div class="menu--container">
            <div class="menu--subcontainer">
                <div class="menu--items">
                    <ul class="menu--logo">
                        <li><a href="/">ACME Store</a></li>
                    </ul>
                    <ul class="menu--links-top">
                        <li>New Products</li>
